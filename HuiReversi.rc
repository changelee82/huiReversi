        MENUITEM SEPARATOR
        MENUITEM "退出(&X)",                    ID_APP_EXIT
    END
    POPUP "设置(&C)"
    BEGIN
        MENUITEM "玩家先走(&F)",                IDM_SET_FIRST_PLAYER
        MENUITEM "电脑先走(&S)",                IDM_SET_FIRST_COM
        MENUITEM "双人对战(&H)",                IDM_SET_VS_PLAYER
        MENUITEM "电脑对战(&C)",                IDM_SET_VS_COM
        MENUITEM "网络对战(&U)",                IDM_SET_VS_NET
        MENUITEM SEPARATOR
        MENUITEM "本机IP(&B)",                  IDM_GET_IP
        MENUITEM "断开连接(&D)",                IDM_SET_DISCONN
        MENUITEM SEPARATOR
        MENUITEM "开局模式1",                   IDM_SET_STARTMODE1
        MENUITEM "开局模式2",                   IDM_SET_STARTMODE2
        MENUITEM "开局模式3",                   IDM_SET_STARTMODE3
        MENUITEM "开局模式4",                   IDM_SET_STARTMODE4
        MENUITEM SEPARATOR
        POPUP "难度设置"
        BEGIN
            MENUITEM "简单",                        IDM_SET_LEVEL1
            MENUITEM "初级",                        IDM_SET_LEVEL2
            MENUITEM "中级",                        IDM_SET_LEVEL3
            MENUITEM "高级",                        IDM_SET_LEVEL4
            MENUITEM "终极",                        IDM_SET_LEVEL5
        END
        MENUITEM "动画效果(&Y)",                IDM_SET_FLASH
        MENUITEM "电脑提示",                    IDM_SET_PROMPT
    END
    POPUP "查看(&V)"
    BEGIN
        MENUITEM "工具栏(&T)",                  ID_VIEW_TOOLBAR
        MENUITEM "状态栏(&S)",                  ID_VIEW_STATUS_BAR
        MENUITEM SEPARATOR
        MENUITEM "上边标题",                    ID_VIEW_TOP
        MENUITEM "左边标题",                    ID_VIEW_LEFT
        MENUITEM SEPARATOR
        MENUITEM "[&Z]放大",                    ID_VIEW_ZOOMIN
        MENUITEM "[&M]缩小",                    ID_VIEW_ZOOMOUT
    END
    POPUP "帮助(&H)"
    BEGIN
        MENUITEM "游戏规则...",                 IDM_HELP_RULE
        MENUITEM "关于 HuiReversi(&A)...",      ID_APP_ABOUT
    END
END


