    BEGIN
        MENUITEM "新游戏(&N)\tCtrl+N",          ID_FILE_NEW
        MENUITEM "载入游戏(&O)...\tCtrl+O",     ID_FILE_OPEN
        MENUITEM "保存游戏(&S)\tCtrl+S",        ID_FILE_SAVE
